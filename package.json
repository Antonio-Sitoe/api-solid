    "solid",
    "node"
  ],
  "author": "antoniositoehl@gmail.com",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "9.17.0",
    "@types/bcryptjs": "2.4.6",
    "@types/node": "^22.10.3",
