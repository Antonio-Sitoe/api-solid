    "@vitest/ui": "2.1.8",
    "eslint": "9.17.0",
    "globals": "15.14.0",
    "prettier": "3.4.2",
    "prisma": "6.1.0",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "8.19.0",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "2.1.8"
  },
  "dependencies": {
    "@fastify/jwt": "9.0.3",
